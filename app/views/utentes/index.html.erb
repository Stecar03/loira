<p id="notice"><%= notice %></p>

<h1>Utentes</h1>

<table>
  <thead>
    <tr>
      <th>Cognome</th>
      <th>Nome</th>
      <th>Indirizzo</th>
      <th>Numero</th>
      <th>Annotazioni</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utentes.each do |utente| %>
      <tr>
        <td><%= utente.cognome %></td>
        <td><%= utente.nome %></td>
        <td><%= utente.indirizzo %></td>
        <td><%= utente.numero %></td>
        <td><%= utente.annotazioni %></td>
        <td><%= link_to 'Show', utente %></td>
        <td><%= link_to 'Edit', edit_utente_path(utente) %></td>
        <td><%= link_to 'Destroy', utente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Utente', new_utente_path %>
